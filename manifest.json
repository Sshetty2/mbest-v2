{
  "manifest_version": 3,
  "name": "Meetup Batch-Event-Set Tool v2",
  "short_name": "MBEST-v2",
  "description": "This extension allows you to schedule multiple Meetup.com events at once",
  "version": "1.0.0",
  "icons": {
    "16": "src/assets/meetup-icon-white-black.png",
    "48": "src/assets/meetup-logo-small.png",
    "128": "src/assets/meetup-logo-small.png"
  },
  "permissions": [
    "identity",
    "storage",
    "background"
  ],
  "content_scripts": [
    {
      "js": [
        "src/content.tsx"
      ],
      "matches": [
        "https://*.meetup.com/home/*"
      ]
    }
  ],
  "oauth2": {
    "client_id": "862472925893-gbhuqd7sm97h0fs3dv5p2a9875l95v6d.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/calendar.events"
    ]
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "src/assets/*"
      ],
      "matches": []
    }
  ],
  "content_security_policy": {},
  "host_permissions": [
    "https://*.meetup.com/*"
  ]
}